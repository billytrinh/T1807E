                        <div class="contact-left ">
                            <div class="left-1">
                                <div class="image_wrapper">
                                    <img src="../images/contact1.jpg" class="contact1 wow bounceInLeft" data-wow-delay="0.1s" data-wow-offset="100">
                                </div>
                            </div>

                        </div>
                        <div class="contact-right wow bounceInRight" data-wow-delay="0.1s" data-wow-offset="100">
                            <div class="column mcb-column one column_column ">
                                <div class="column_attr clearfix align_center">
                                    <h3>Feel free and write</h3>
                                    <p class="to1">Online contact</p>
                                </div>
                            </div>
                            <div class="column mcb-column two-third column_column ">
                                <div class="column_attr clearfix">
                                    <div class="wpcf7">
                                        <form action="/admin/message" method="POST" class="wpcf7-form">
                                            <div class="column one-second">
                                                <span class="wpcf7-form-control-wrap your-name">
                                                    <input type="text" name="customer_name" size="40" required class="wpcf7-form-control wpcf7-text wpcf7-validates-as-required"
                                                        placeholder="Your name">
                                                </span>
                                            </div>
                                            <div class="column one-second">
                                                <span class="wpcf7-form-control-wrap your-email">
                                                    <input type="email" name="email" size="40" required class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email"
                                                        placeholder="Your e-mail">
                                                </span>
                                            </div>
                                            <div class="column one">
                                                <span class="wpcf7-form-control-wrap your-subject">
                                                    <input type="text" name="subject" value="" size="40" class="wpcf7-form-control wpcf7-text"
                                                        placeholder="Subject">
                                                </span>
                                            </div>
                                            <div class="column one">
                                                <div class="wpcf7-form-control-wrap your-message">
                                                    <textarea name="message" cols="20" rows="7" class="wpcf7-form-control wpcf7-textarea"
                                                        placeholder="Message"></textarea>
                                                </div>
                                            </div>
                                            <div class="column one">
                                                <input type="submit" value="Send a message" class="wpcf7-form-control wpcf7-submit button_full_width">
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 cuoicung">
                <div class="container">
                    <div class="contact-left1">
                        <div class="column mcb-column one-second column_column ">
                            <div class="omchu">
