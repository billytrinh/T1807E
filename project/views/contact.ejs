                                <% if(success_msg.length >0){ %>
                                <div class="alert alert-success text-center wow bounceInLeft col-sm-12 " data-wow-delay="0.1" role="alert">
                                    <strong>
                                        <%= success_msg %></strong>
                                </div>
                                <%} %>
                        
                            </div>
                        </div>
                <div class="container ">
                    <div class="phanchung">
                        <p class="to wow bounceInLeft" data-wow-delay="0.1s">Let's Talk</p>
